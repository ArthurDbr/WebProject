{% autoescape %}
    <rss version="2.0">
        <channel>
            <title> {{ titre }} </title>
            <link> {{ link }} </link>
            <description> {{ description }} </description>
            {% for event in evenement %}
                <item>
                    <title> {{ event.description }} </title>
                    <link> {{ linkEvent }}{{ event.id }} </link>
                    <description> {{ event.dateEvenement }}  {{ event.heureEvenement }} </description>
                </item>
            {% endfor %}
        </channel>
    </rss>
{% endautoescape %}