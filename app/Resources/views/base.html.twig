<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Community{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('img/Logo.svg') }}" />
        <link href="{{ asset('css/bootstrapAccueil.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/Accueil.css') }}" rel="stylesheet">

    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
          <a class="navbar-brand" href="{{ path('accueil_index') }}">Community </a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>

          <div class="collapse navbar-collapse" id="navbarColor01">
            <ul class="navbar-nav mr-auto">

              <li class="nav-item active">
                <a class="nav-link" href="{{ path('createEvent') }}">Create event</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="{{ path('MyEvent') }}">My Event </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="{{ path('MyProfil') }}">My Profil</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">About</a>
              </li>
            </ul>
          </div>
        </nav>
        
  
        {% block body %}
        {% endblock %}
        {% block javascripts %}
          <script src="{{ asset('js/bootstrap.js') }}" type="text/javascript"></script>
          <script src="{{ asset('js/jquery.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
